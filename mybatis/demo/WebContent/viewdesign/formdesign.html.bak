                        <div class="pull-left">
						    <div class="pull-left bg-info" style="width:60px;text-align:center">
							<a style="font-size:18px;font-weight:bold;color:#ffffff;line-height:50px">KB<span style="color:#ADFF2F;font-weight:bold">i</span>S</a>
							</div>
                            <button type="button" class="btn btn-default navbar-btn" onclick="fd.design._new('#containers')">
                                <i class="fa fa-plus-square-o"></i> 新 建
                            </button>
                            <button type="button" class="btn btn-default navbar-btn" onclick="fd.design.updata()">
                                <i class="fa fa-save"></i> 保 存
                            </button>
                            <button type="button" class="btn btn-default navbar-btn" data-toggle="modal" data-target="#formModal" id="save-btn">
                                <i class="fa fa-save"></i> 另 存 为
                            </button>
                            <div class="btn-group navbar-btn">
                                <button type="button" class="btn btn-default dropdown-toggle navbar-btn" data-toggle="dropdown">
                                    更 多 <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li onclick="fd.design.load_reset()"><a data-toggle="modal" data-target="#resetModal" style="cursor:pointer;"><i class="fa fa-history"></i> 导 入</a></li>
                                    <li><a href="javascript:;" onclick="fd.design.preview()"><i class="fa fa-eye"></i> 预 览</a></li>
                                    <li><a href="javascript:;" onclick="fd.design.rest()" style="color:red;"><i class="fa fa-rotate-left"></i> 重 做</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="pull-right">
                            <div class="btn-group navbar-btn">
                                <button type="button" class="btn btn-default dropdown-toggle navbar-btn" data-toggle="dropdown">
                                    <i class="fa fa-arrows"></i> 移动布局 <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="javascript:;" onclick="fd.design.move(&#39;up&#39;)"><i class="fa fa-arrow-circle-up"></i> 向上(左)移动</a></li>
                                    <li><a href="javascript:;" onclick="fd.design.move(&#39;down&#39;)"><i class="fa fa-arrow-circle-down"></i> 向下(右)移动</a></li>
                                </ul>
                            </div>
                            <button type="button" class="btn btn-default navbar-btn" onclick="fd.design.remove()">
                                <i class="fa fa-trash"></i> 删除选中
                            </button>
                            <div class="btn-group navbar-btn">
                                <button type="button" class="btn btn-default dropdown-toggle navbar-btn" data-toggle="dropdown">
                                    <i class="fa fa-table"></i> 表格操作 <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="javascript:;" onclick="fd.design.table.merge()"><i class="fa fa-columns"></i> 合并单元格</a></li>
                                    <li><a href="javascript:;" onclick="fd.design.table.insertRow(&#39;up&#39;)"><i class="fa fa-th"></i> 上方增加一行</a></li>
                                    <li><a href="javascript:;" onclick="fd.design.table.insertRow(&#39;down&#39;)"><i class="fa fa-th"></i> 下方增加一行</a></li>
